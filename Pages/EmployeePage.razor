@page "/employeepage"
<h3>Daftar Employee</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Employee Id</th>
            <th>Name</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var empl in Employees)
        {
            <tr>
                <td>@empl.EmployeeId</td>
                <td>@empl.FirstName @empl.LastName</td>
                <td><a href="@($" employeedetail/{empl.EmployeeId}")" class="btn btn-primary">detail</a></td>
            </tr>
        }
    </tbody>
</table>